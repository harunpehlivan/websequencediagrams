<Application x:Class="Vurdalakov.WebSequenceDiagrams.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Vurdalakov.WebSequenceDiagrams"
             xmlns:vurdalakov="clr-namespace:Vurdalakov"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <vurdalakov:EnumToBooleanConverter x:Key="EnumToBooleanConverter" />
        <vurdalakov:NullToHiddenConverter x:Key="NullToHiddenConverter" />
        <vurdalakov:NullToFalseConverter x:Key="NullToFalseConverter" />
        <vurdalakov:ReverseBooleanConverter x:Key="ReverseBooleanConverter" />
        <vurdalakov:ZeroToFalseConverter x:Key="ZeroToFalseConverter" />
        <vurdalakov:FormatMenuItemHeaderConverter x:Key="FormatMenuItemHeaderConverter" />
    </Application.Resources>
</Application>
